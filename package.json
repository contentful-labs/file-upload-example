{
  "name": "file-upload-example",
  "version": "0.0.0",
  "description": "contentful CMA file upload demonstration",
  "main": "index.js",
  "author": "Benedikt RÃ¶tsch <info@benedikt-roetsch.de>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build:babel && npm run build:webpack",
    "build:babel": "NODE_ENV=production babel src --out-dir dist --ignore src/app",
    "build:webpack": "NODE_ENV=production webpack --progress",
    "build:webpack:log": "NODE_ENV=production webpack --json --profile --progress > webpack-build-log.json && webpack-bundle-analyzer webpack-build-log.json dist/app",
    "clean": "rimraf dist",
    "deploy": "npm run build && gh-pages -d dist/app",
    "dev": "NODE_ENV=development nodemon src/scripts/dev-server.js --exec babel-node --watch src/scripts/dev-server.js --watch config --watch webpack.config.js",
    "lint": "eslint src",
    "prod": "NODE_ENV=production node dist/scripts/prod-server.js",
    "pretest": "npm run build",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:unit": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test prod http://localhost:8080/file-upload-example/ test:e2e:run",
    "test:e2e:dev": "start-server-and-test prod http://localhost:8080/file-upload-example/ test:e2e:open",
    "test:e2e:run": "cypress run --env spaceId=$FILE_UPLOAD_E2E_SPACE_ID,accessToken=$FILE_UPLOAD_E2E_CMA_TOKEN",
    "test:e2e:open": "cypress open --env spaceId=$FILE_UPLOAD_E2E_SPACE_ID,accessToken=$FILE_UPLOAD_E2E_CMA_TOKEN"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "devDependencies": {
    "babel-cli": "7.0.0-beta.3",
    "babel-core": "7.0.0-beta.3",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-node": "7.0.0-beta.3",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "7.0.0-beta.3",
    "babel-preset-env": "7.0.0-beta.3",
    "babel-preset-preact": "^1.1.0",
    "codecov": "^3.0.0",
    "css-loader": "^0.28.10",
    "cypress": "^2.0.4",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-preact": "^1.1.6",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-cypress": "^2.0.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.10",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "jest-serializer-html-string": "^1.0.1",
    "koa": "^2.5.0",
    "koa-compress": "^2.0.0",
    "koa-mount": "^3.0.0",
    "koa-static": "^4.0.2",
    "lodash-webpack-plugin": "^0.11.4",
    "nodemon": "^1.14.3",
    "postcss": "^6.0.19",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.1",
    "preact-render-to-string": "^3.7.0",
    "redux-devtools": "^3.4.1",
    "rimraf": "^2.6.0",
    "sane-config": "^1.0.0",
    "start-server-and-test": "^1.0.1",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.11.0",
    "webpack-dev-server": "^3.0.0"
  },
  "dependencies": {
    "contentful-management": "^5.0.0-beta0",
    "date-fns": "^1.28.5",
    "history": "^4.6.3",
    "normalize.css": "^8.0.0",
    "preact": "^8.2.7",
    "preact-async-route": "^2.1.0",
    "preact-compat": "^3.18.0",
    "preact-redux": "^2.0.3",
    "preact-router": "^2.6.0",
    "proptypes": "^1.1.0",
    "react-dropzone": "^4.2.8",
    "redux": "^3.6.0",
    "redux-saga": "^0.16.0",
    "redux-storage": "^4.1.2",
    "redux-storage-decorator-debounce": "^1.1.3",
    "redux-storage-decorator-filter": "^1.1.8",
    "redux-storage-engine-localstorage": "^1.1.4",
    "sweetalert": "^1.1.3"
  },
  "jest": {
    "snapshotSerializers": [
      "jest-serializer-html-string"
    ],
    "collectCoverageFrom": [
      "src/app/**/*.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/app"
    ],
    "moduleNameMapper": {
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
