  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build:babel && npm run build:webpack",
    "build:babel": "NODE_ENV=production babel src --out-dir dist --ignore src/app",
    "build:webpack": "NODE_ENV=production webpack --progress",
    "build:webpack:log": "NODE_ENV=production webpack --json --profile --progress > webpack-build-log.json && webpack-bundle-analyzer webpack-build-log.json dist/app",
    "clean": "rimraf dist",
    "deploy": "npm run build && gh-pages -d dist/app",
    "dev": "NODE_ENV=development nodemon src/scripts/dev-server.js --exec babel-node --watch src/scripts/dev-server.js --watch config --watch webpack.config.js",
    "lint": "eslint src",
    "prod": "NODE_ENV=production node dist/scripts/prod-server.js",
    "test": "npm run lint && npm run build"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "devDependencies": {
